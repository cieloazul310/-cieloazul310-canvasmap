{
  "name": "@cieloazul310/canvasmap",
  "description": "Generate plain map for data visualization.",
  "version": "0.2.0",
  "main": "lib/index.js",
  "module": "module/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./module/index.d.ts",
        "default": "./module/index.js"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      },
      "default": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "lib",
    "module"
  ],
  "license": "MIT",
  "author": {
    "name": "@cieloazul310",
    "url": "https://cieloazul310.github.io"
  },
  "keywords": [
    "Data Visualization",
    "D3.js",
    "Canvas"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "npm-run-all dev:mjs dev:cjs --parallel",
    "dev:mjs": "tsc --watch -p ./tsconfig.mjs.json",
    "dev:cjs": "tsc --watch -p ./tsconfig.json",
    "clean": "git clean -fx lib/ module/",
    "tsc": "tsc -p ./tsconfig.json && tsc -p ./tsconfig.mjs.json",
    "build": "npm-run-all clean tsc",
    "check-types": "tsc --noEmit",
    "eslint": "eslint src/**/*.ts --fix",
    "lint": "npm-run-all eslint check-types"
  },
  "dependencies": {
    "@cieloazul310/canvasmap-styles": "^0.2.0",
    "@cieloazul310/canvasmap-utils": "^0.2.0",
    "@turf/bbox": "^6.5.0",
    "@turf/bbox-polygon": "^6.5.0",
    "@turf/helpers": "^6.5.0",
    "@turf/rewind": "^6.5.0",
    "canvas": "^2.11.2",
    "d3-geo": "^2.0.1",
    "d3-tile": "^1.0.0"
  },
  "devDependencies": {
    "@cieloazul310/eslint-config-custom": "^0.2.0",
    "@cieloazul310/tsconfig": "^0.2.0",
    "@types/d3-geo": "^2.0.1",
    "typescript": "^5.2.2"
  }
}